 <nav aria-label="Page navigation example">
  <ul class="pagination">
   {% if page > 1  %}
    <li class="page-item">
      <a class="page-link" href="{{ url('products',{'page': page - 1}) }}" aria-label="Previous">
        <span aria-hidden="true">&laquo;</span>
      </a>
    </li>
        <li class="page-item"><a class="page-link" href="{{ url('products',{'page': page - 1}) }}">{{ page - 1 }}</a></li>
    {% endif %}
    <li class="page-item"><a class="page-link" href="{{ url('products',{'page': page}) }}">{{ page }}</a></li>
    {% if page < 5 %}
    <li class="page-item"><a class="page-link" href="{{ url('products',{'page': page + 1}) }}">{{ page + 1 }}</a></li>
     <li class="page-item">
      <a class="page-link" href="{{ url('products',{'page': page + 1}) }} " aria-label="Next">
        <span aria-hidden="true">&raquo;</span>
      </a>
    </li>
    {% endif %}
   
  </ul>
</nav>